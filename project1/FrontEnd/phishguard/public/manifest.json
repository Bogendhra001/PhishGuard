{
  "short_name": "PhishGuard",
  "name": "PhishGuard",
  "description": "Url analyzser",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "PhishGuard.png",
      "128": "PhishGuard.png"
    }
    
  }
}